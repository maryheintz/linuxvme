#gcc -c gdttf.c
gcc -c gdcache.c
gcc -c gdkanji.c
gcc -c wbmp.c
gcc -c gd_wbmp.c
gcc -c gdhelpers.c

rm libgd.a
ar -r libgd.a gd.o
ar -r libgd.a gd_gd.o
ar -r libgd.a gd_gd2.o
ar -r libgd.a gd_io.o
ar -r libgd.a gd_io_dp.o
ar -r libgd.a gd_io_file.o
ar -r libgd.a gd_security.o
ar -r libgd.a gd_ss.o
ar -r libgd.a gd_io_ss.o
ar -r libgd.a gd_png.o
ar -r libgd.a gd_jpeg.o
ar -r libgd.a gdxpm.o
ar -r libgd.a gdfontt.o
ar -r libgd.a gdfonts.o
ar -r libgd.a gdfontmb.o
ar -r libgd.a gdfontl.o
ar -r libgd.a gdfontg.o
ar -r libgd.a gdtables.o
ar -r libgd.a gdft.o
#ar -r libgd.a gdttf.o
ar -r libgd.a gdcache.o
ar -r libgd.a gdkanji.o
ar -r libgd.a wbmp.o
ar -r libgd.a gd_wbmp.o
ar -r libgd.a gdhelpers.o

ar -s libgd.a
cp -r libgd.a /home/user/kelby/lib

