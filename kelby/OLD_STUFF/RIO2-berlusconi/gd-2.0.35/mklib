gcc -c gd_jpeg.c
gcc -c gdxpm.c
gcc -c gdfontt.c
gcc -c gdfonts.c
gcc -c gdfontmb.c
gcc -c gdfontl.c
gcc -c gdfontg.c
gcc -c gdtables.c
