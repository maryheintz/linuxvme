ar -r libkja.a dot.o
ar -r libkja.a dxy.o
ar -r libkja.a hst.o
ar -r libkja.a post.o
ar -r libkja.a ranf.o

ar -s libkja.a
cp -r libkja.a /home/user/kelby/lib

